{
  "name": "tests-buster",
  "version": "1.0.0",
  "description": "clear broken tests from your test files at no time",
  "main": "tests-buster.js",
  "scripts": {
    "test": "test",
    "bust": "babel-node tests-buster.js",
    "lint": "eslint . --fix",
    "prepublish": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-generator-functions"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Efrat19/tests-buster.git"
  },
  "keywords": [
    "jest",
    "clear",
    "test",
    "unit",
    "remove",
    "broken"
  ],
  "author": "Efrat19",
  "license": "ISC",
  "bin": {
    "tests-buster": "./src/index.js"
  },
  "dependencies": {
    "clui": "^0.3.6",
    "fs": "0.0.1-security",
    "minimist": "^1.2.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0"
  }
}
