{
  "name": "tests-buster",
  "version": "1.0.0",
  "description": "clear broken tests from your test files at no time",
  "main": "tests-buster.js",
  "scripts": {
    "test": "test",
    "bust": "babel src --out-dir lib && tests-buster",
    "bust:dev": "babel-node tests-buster.js",
    "bust:offline": "npm run prepublish && node lib/index.js",
    "lint": "eslint . --fix",
    "prepublish": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Efrat19/tests-buster.git"
  },
  "keywords": [
    "jest",
    "clear",
    "test",
    "unit",
    "remove",
    "broken"
  ],
  "author": "Efrat19",
  "license": "ISC",
  "bin": {
    "tests-buster": "./lib/index.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "clui": "^0.3.6",
    "fs": "0.0.1-security",
    "minimist": "^1.2.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0"
  }
}
